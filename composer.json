{
    "name": "aschmelyun/larametrics",
    "description": "A self-hosted metrics and notifications platform for Laravel apps",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Schmelyun",
            "email": "me@aschmelyun.com",
            "homepage": "https://aschmelyun.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "autoload": {
        "psr-4": {
            "Aschmelyun\\Larametrics\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aschmelyun\\Larametrics\\LarametricsServiceProvider"
            ]
        }
    }
}
